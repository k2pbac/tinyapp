<%- include('./partials/_header') %>

<div class="row">
  <div class="col-8 offset-3">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading">Error <%= err.status %></h4>
      <p><%= err.message %></p>
      <% if (process.env.NODE_ENV !=="production" ) { %>
      <p><%= err.stack %></p>
      <% } %>
    </div>
  </div>
</div>
<%- include('./partials/_footer') %>
